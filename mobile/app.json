{
  "expo": {
    "name": "nlw-copa",
    "slug": "nlw-copa",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "com.brunocs90.nlwcopa",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
